pipeline {
    agent any
    
    stages {
        stage('Checkout Code') {
            steps {
            git branch:'master', url: 'https://github.com/GhanaATS/eureka.git'
            }
        }
    }
  stages {
        stage('build') {
            steps {
             sh "docker build -t eureka ."

            }
        }
    }
}
